{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "custom-blocks/banner-block",
  "version": "0.1.0",
  "title": "Banner Block",
  "category": "theme",
  "icon": "analytics",
  "description": "Cecils banner block",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "imageSrc": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "imageAlt": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "alt"
    },
    "headlineText": {
      "type": "string"
    },
    "ctaText": {
      "type": "string"
    },
    "ctaLink": {
      "type": "string",
      "default": "#"
    }
  },
  "textdomain": "banner-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}